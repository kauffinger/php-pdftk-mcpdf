{
    "name": "kauffinger/php-pdftk-mcpdf",
    "description": "A PDF conversion and form utility based on mikehaertl/pdftk and vigicorp/php-mcpdf. Dynamically usable with mcpdf.",
    "keywords": ["pdf", "pdftk"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Konstantin Auffinger",
            "email": "konstantinauffinger@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "mikehaertl/php-shellcommand": "^1.6.3",
        "mikehaertl/php-tmpfile": "^1.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">4.0 <9.4"
    },
    "autoload": {
        "psr-4": {
            "kauffinger\\pdftkmcpdf\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests"
        }
    }
}
